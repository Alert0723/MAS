<UserControl x:Class="Adaptable_Studio.KeepButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="75" Loaded="UserControl_Loaded">
    <Grid Background="#02000000" MouseDown="Path_MouseDown">
        <Path x:Name="PathControl" Stretch="Uniform" Fill="White" Margin="2"/>
    </Grid>
</UserControl>
